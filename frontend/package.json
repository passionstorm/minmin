    "dev:admin": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config ./webpack_ad.config.js",
    "dev:front": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config ./webpack_front.config.js",
    "watch": "npm-run-all watch:*",
    "build": "npm-run-all build:*"
  },
